{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 90,
    "bracketSpacing": true,
    "insert_final_newline": true,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "trailingComma": "es5",
                "tabWidth": 4
            }
        },
        {
            "files": "*.tsx",
            "options": {
                "trailingComma": "es5",
                "tabWidth": 4,
                "jsxSingleQuote": false,
                "embeddedLanguageFormatting": "auto"
            }
        }
    ]
}
